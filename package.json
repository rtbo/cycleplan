{
  "name": "cycleplan",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vite preview",
    "build": "vite build",
    "tslint": "tsc --noemit",
    "tslint:vue": "vti diagnostics",
    "eslint": "eslint . --ext .js,.ts,.vue --max-warnings 0",
    "eslint:fix": "eslint . --ext .js,.ts,.vue --fix",
    "lint": "npm-run-all --parallel tslint tslint:vue eslint",
    "test": "ts-mocha -p tsconfig.test.json -r tsconfig-paths/register src/**/*.spec.ts tests/unit/**/*.ts",
    "dev": "vite"
  },
  "gitHooks": {
    "pre-commit": "eslint . --ext .js,.ts,.vue --max-warnings 0 && tsc --noemit"
  },
  "dependencies": {
    "@fontsource/roboto": "^4.5.0",
    "@mdi/font": "^5.9.55",
    "core-js": "^3.6.5",
    "register-service-worker": "^1.7.1",
    "vue": "^3.0.5",
    "vuex": "^4.0.2"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/mocha": "^5.2.4",
    "@types/splitpanes": "^2.2.1",
    "@typescript-eslint/eslint-plugin": "^4.18.0",
    "@typescript-eslint/parser": "^4.18.0",
    "@vitejs/plugin-vue": "^1.2.4",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.3.1",
    "chai": "^4.1.2",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^6.2.2",
    "husky": "^7.0.1",
    "mocha": "^9.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.6",
    "prettier": "^2.2.1",
    "tailwindcss": "^2.2.7",
    "ts-mocha": "^8.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "~4.1.5",
    "vite": "^2.4.2",
    "vite-svg-loader": "^2.1.0",
    "vti": "^0.1.3",
    "vue-tsc": "^0.0.24"
  }
